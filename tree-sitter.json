{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [{
    "name": "rush",
    "camelcase": "Rush",
    "title": "rush",
    "scope": "source.rush",
    "file-types": ["rush"],
    "injection-regex": "^rush$",
    "class-name": "TreeSitterRush",
    "highlights": "queries/rush/highlights.scm",
    "locals": "queries/rush/locals.scm"
  }],
  "metadata": {
    "version": "0.2.0",
    "license": "MIT",
    "description": "rush grammar for tree-sitter",
    "authors": [{
      "name": "RubixDev",
      "email": "silas.groh@t-online.de"
    }],
    "links": {
      "repository": "https://github.com/rush-rs/tree-sitter-rush"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
